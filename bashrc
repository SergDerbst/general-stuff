#git aliases
alias gs='git status'
alias ga='git add .'
alias gm='git commit -m'
alias gp='git push origin'

#java aliases
alias jps='jpa -l'
alias ra='mvn clean install spring-boot:run'
alias da='mvn clean install spring-boot:run -Ddebug'

#sys aliases
alias ll='ls -la'
alias cd..='cd ..'
alias dev='cd ~/Development'
alias reload='. ~/.bashrc'
